---
interface Props {
  program: Program;
}

interface Program {
  title: string;
  description: string;
}

const { title, description } = Astro.props.program;
---

<div class="flex1 mb-2">
    <h2 class="text-2xl font-bold text-cyan-800">{title}</h2>
    <p class="text-gray-400 text-sm">{description}</p>
</div>